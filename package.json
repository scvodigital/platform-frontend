{
  "name": "@scvo/frontend",
  "version": "0.0.1",
  "description": "The App Engine service that runs the SCVO=>Router",
  "main": "build/index.js",
  "engines": {
    "node": ">=8.11.1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\"",
    "cleanup": "rm -rf ./build/**",
    "copy": "cp ./db/db.json ./build/db.json; cp -r ./assets ./build/assets",
    "sass": "node_modules/node-sass/bin/node-sass --include-path=./node_modules ./build/assets/css -o build/assets/css",
    "upgrade-router": "yarn add -E @scvo/router @scvo/router-task-firebase @scvo/router-task-elasticsearch @scvo/router-destination-handlebars @scvo/router-destination-redirect",
    "build": "yarn; npm run cleanup; npm run compile; npm run build-db; npm run copy; npm run sass; npm run uglify",
    "build-db": "node_modules/@scvo/json-inc/bin/json-inc compile --input ./sites/sites.inc.json --output ./sites/sites.json",
    "start": "npm run build; node build/index.js",
    "dev": "npm run build; devmode=true node build/index.js",
    "dev-router": "npm run router; npm run dev",
    "deploy-dispatch": "gcloud app deploy dispatch.yaml --quiet",
    "clear-cache": "curl -s https://scvo.org.uk/reload-sites",
    "deploy-db": "npm run build-db; firebase database:set -y /contexts ./sites/sites.json; npm run clear-cache",
    "deploy-app": "npm run build; npm run deploy-dispatch; gcloud beta app deploy --quiet; npm run deploy-db",
    "router": "cd ../router && npm run local && cd ../frontend && npm run l-router && npm run l-elastic && npm run l-handlebars && npm run l-redirect && npm run l-firebase",
    "l-router": "npm link @scvo/router",
    "l-firebase": "npm link @scvo/router-task-firebase",
    "l-elastic": "npm link @scvo/router-task-elasticsearch",
    "l-handlebars": "npm link @scvo/router-destination-handlebars",
    "l-redirect": "npm link @scvo/router-destination-redirect",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "node_modules/typescript/bin/tsc -p .",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check",
    "uglify": "npm run uglify-main; npm run uglify-search; npm run uglify-home",
    "uglify-main": "uglifyjs --compress --output=./build/assets/js/goodmoves/main.js ./build/assets/js/jquery-deserialize.js ./build/assets/js/cookieinfo.min.js ./build/assets/js/class-inheritance.js ./build/assets/js/goodmoves/site-controller.js",
    "uglify-home": "uglifyjs --compress --output=./build/assets/js/goodmoves/home.js ./build/assets/js/goodmoves/typeahead-controller.js ./build/assets/js/goodmoves/home-controller.js",
    "uglify-search": "uglifyjs --compress --output=./build/assets/js/goodmoves/search.js ./build/assets/js/goodmoves/typeahead-controller.js ./build/assets/js/goodmoves/search-controller.js"
  },
  "author": "Matthew Wilkes (https://scvo.org.uk)",
  "license": "GPL-3.0",
  "dependencies": {
    "@scvo/router": "^1.2.6",
    "@scvo/router-destination-handlebars": "0.0.29",
    "@scvo/router-destination-redirect": "0.0.7",
    "@scvo/router-task-elasticsearch": "0.0.16",
    "@scvo/router-task-firebase": "0.0.2",
    "@scvo/router-task-mysql": "^0.0.3",
    "@scvo/router-task-transform": "^0.0.3",
    "@types/cookie-parser": "^1.4.1",
    "body-parser": "^1.18.2",
    "chalk": "^2.3.1",
    "clayhandlebars": "^3.2.3",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "date-fns": "^1.29.0",
    "dot-object": "^1.7.0",
    "express": "^4.16.2",
    "firebase-admin": "^5.12.1",
    "googleapis": "^30.0.0",
    "gts": "^0.5.4",
    "handlebars": "^4.0.11",
    "jsforce": "^1.8.4",
    "livereload": "^0.7.0",
    "moment": "^2.22.2",
    "nsfw": "^1.0.16",
    "pdfmake": "^0.1.36",
    "string": "^3.3.3",
    "typescript": "^2.9.2",
    "@scvo/json-inc": "^0.0.14",
    "@types/body-parser": "^1.16.8",
    "@types/chalk": "^2.2.0",
    "@types/compression": "^0.0.35",
    "@types/cors": "^2.8.3",
    "@types/dot-object": "^1.5.0",
    "@types/elasticsearch": "^5.0.22",
    "@types/express": "^4.11.1",
    "@types/handlebars": "^4.0.38",
    "@types/jsforce": "^1.8.10",
    "@types/node": "^9.6.5",
    "@types/q": "^1.0.7",
    "@types/request": "^2.47.0",
    "@types/string": "^0.0.29",
    "material-components-web": "^0.35.0",
    "node-sass": "^4.7.2"
  }
}
