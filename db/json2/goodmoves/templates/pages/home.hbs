<section class="search">
    <div class="container-fluid">
            <h3>Find you new job?</h3>
            <div class="mdc-layout-grid">
                <div class="mdc-layout-grid__inner">
                    <div id="tt-what" class="mdc-layout-grid__cell mdc-layout-grid__cell--span-6-desktop mdc-layout-grid__cell--span-8-tablet mdc-layout-grid__cell--span-4-phone">
                        <input type="text" class="mdc-textfield__input white-text mdc-text-field searchRoute typeahead" placeholder="What are you looking for" id="what" name="what">
                    </div>
                    <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-6-desktop mdc-layout-grid__cell--span-8-tablet mdc-layout-grid__cell--span-4-phone">
                        <input type="text" class="mdc-textfield__input white-text mdc-text-field searchRoute" placeholder="Where are you looking" id="where" name="where">
                    </div>
                </div>
            </div>
            <div id="search-terms" class="mdc-chip-set"></div>
    </div>
</section>
<section class="search-results">
    <div id="map-full">

    </div>
    <div class="container-fluid">
        <div id="search-results-info">
            <strong>Total:</strong> <span id="search-results-total"></span>
        </div>
        <div id="search-results"></div>
    </div>
</section>
