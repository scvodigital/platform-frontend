<div class="event event-booking">
    {{> header_splash_event}}
    <div class="container-fluid page-content">
        {{#with data.primaryResponse.hits.hits.[0]._source ~}}
            <ul class="breadcrumbs">
                <li><a href="/">Home</a></li>
                <li><a href="/jobs">Jobs</a></li>
                <li><a href="/jobs/community-jobs-scotland">Community Jobs Scotland</a></li>
                <li><a href="/jobs/community-jobs-scotland/training-opportunities">Training opportunities</a></li>
                <li><a href="/jobs/community-jobs-scotland/training-opportunities/{{slug}}">{{{title}}}</a></li>
                <li>Booking</li>
            </ul>
            <div class="mdc-layout-grid">
                <div class="mdc-layout-grid__inner">
                    <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-9-desktop mdc-layout-grid__cell--span-8-tablet mdc-layout-grid__cell--span-4-phone content event-content">
                        <p><strong>BOOKING FORM NOT YET SET UP</strong></p>

                        {{#compare course_type "===" "Training Course" ~}}
                        <p>
                            <strong><a href="https://storage.googleapis.com/scvo-cms/wp-content/uploads/2018/01/Policies-procedures-and-TCs.docx" download>Policies, procedures and terms &amp; conditions</a></strong>
                        </p>
                        {{else}}
                        <p>
                            <strong><a href="https://storage.googleapis.com/scvo-cms/wp-content/uploads/2018/01/Policies-procedures-and-TCs.docx" download>Policies, procedures and terms &amp; conditions</a></strong>
                        </p>
                        {{/compare ~}}
                    </div>
                </div>
            </div>
        {{/with ~}}
    </div>
</div>
