{
    "home": {
        "pattern": "/",
        "destination": {
            "destinationType": "handlebars",
            "config": {
                "default": {
                    "head": `{:templates/head-tags.hbs}`,
                    "body": `{:templates/home.hbs}`,
                    "foot": ""
                }
            }
        },
        "metaData": {
            "title": "Home"
        },
        "tasks": [
            {
                "name": "primaryResponse",
                "taskType": "elasticsearch",
                "config": {
                    "connectionStringTemplate": "https://readonly:onlyread@50896fdf5c15388f8976945e5582a856.eu-west-1.aws.found.io",
                    "elasticsearchConfig": {
                        "apiVersion": "5.4",
                        "log": "trace"
                    },
                    "queryTemplates": [
                        {
                            "name": "first_query",
                            "index": "web-content-production",
                            "type": "goodmoves-job",
                            "template": `{:queries/pages/none.json}`
                        },
                        {
                            "name": "second_query",
                            "index": "web-content-production",
                            "type": "goodmoves-job",
                            "template": `{:queries/pages/none.json}`
                        }
                    ]
                }
            }
        ]
    },
    "_default": {
        "pattern": "/*path",
        "destination": {
            "destinationType": "handlebars",
            "config": {
                "default": {
                    "head": `{:templates/head-tags.hbs}`,
                    "body": `{:templates/404.hbs}`,
                    "foot": ""
                }
            }
        },
        "tasks": [],
        "metaData": {
            "title": "Page Not Found"
        }
    }
}
