{{#if data.primaryResponse.hits.hits.[0] ~}}
{{#with data.primaryResponse.hits.hits.[0]._source ~}}
{{#if title ~}}
<title>Scotland for Europe signatory: {{{title}}}</title>
<meta property="og:title" content="Scotland for Europe signatory: {{{title}}}" data-dynamic="true">
{{else}}
<title>Scotland for Europe</title>
<meta property="og:title" content="Scotland for Europe" data-dynamic="true">
{{/if ~}}
{{#if files.0.uploadUrl ~}}
<meta property="og:image" content="{{files.0.uploadUrl}}" data-dynamic="true">
<meta property="og:image:width" content="128" data-dynamic="true">
<meta property="og:image:height" content="128" data-dynamic="true">
{{/if ~}}
{{#if description ~}}
<meta property="og:summary" content="{{description}}" data-dynamic="true">
<meta name="description" content="{{description}}" data-dynamic="true">
{{/if ~}}
{{#if rendered.json_ld ~}}
<script type="application/ld+json" data-dynamic="true">
{{{rendered.json_ld}}}
</script>
{{/if ~}}
{{/with ~}}
{{else}}
<title>Scotland for Europe</title>
<meta property="og:title" content="Scotland for Europe" data-dynamic="true">
{{/if ~}}