                "function": "index"
            }
        ],
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "headers": [
            {
                "source" : "**/*.@(eot|otf|ttf|ttc|woff|font.css)",
                "headers" : [
                    {
                        "key" : "Access-Control-Allow-Origin",
                        "value" : "*"
                    }
                ]
            },
            {
                "source" : "**",
                "headers" : [
                    {
                        "key" : "Cache-Control",
                        "value" : "max-age=86400"
                    }
                ]
            }
        ]
    },
    "storage": {
        "rules": "firebase-configuration/storage.rules"
    }
