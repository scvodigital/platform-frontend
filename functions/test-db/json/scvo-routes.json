    "main-search": {
        "linkTags": [],
        "metaTags": [],
        "pattern": "/search",
        "template": "<div class=\"container-fluid\">\n    <div class=\"v-margin\">\n        <div class=\"mdc-card \">\n            <section class=\"mdc-card__primary\">\n                <h2 class=\"mdc-card__title mdc-card__title--large\">Search SCVO</h2>\n                <input type=\"text\" class=\"mdc-textfield__input searchRoute\" placeholder=\"Search\" \n                    [searchroute]='\"main-search\"' [paramstemplate]='\"{ \\\"queryParams\\\": { \\\"q\\\": %q } }\"'>\n            </section>\n        </div>\n    </div>\n\n    {{#each primaryResponse.hits.hits}}\n        <div class=\"v-margin\">\n            {{#if _source.rendered.search_result_scvo}}\n                {{{_source.rendered.search_result_scvo}}}\n            {{else}}\n                {{#if _source.rendered.search_result}}\n                    {{{_source.rendered.search_result}}}\n                {{/if}}\n            {{/if}}\n        </div>\n    {{/each}}\n\n    <div class=\"v-margin\">\n        <ul class=\"pagination\">\n            {{#if paging.prevPage}}\n                <li>\n                    <a href=\"\">Previous</a>\n                </li>\n            {{/if}}\n            \n            {{#each paging.pageRange}}\n                <li class=\"{{#compare paging.currentPage \"===\" this}}current-page{{/compare}}\">\n                    <a href=\"\">{{this}}</a>\n                </li>\n            {{/each}}\n\n            {{#if paging.nextPage}}\n                <li>\n                    <a href=\"\">Next</a>\n                </li>\n            {{/if}}\n        </ul>\n    </div>\n</div>\n",
        "jsonLdTemplate": "",
        "primarySearchTemplate": {
            "index": "web-content-production",
            "type": "scvo-cms-page,scvo-cms-post,scvo-member,digital-charter-signatory,goodmoves-legacy,funding-scotland-fund,goodhq-organisation,scvo-document-evidence,scvo-event,scvo-grant-digital,scvo-staff",
            "template": "{\n    \"size\": 10,\n    \"from\": {{multiply (subtract (default query.page 1) 1) 10}},\n    \"query\": {\n        \"query_string\": {\n            \"query\": \"{{query.q}}\",\n            \"default_operator\": \"AND\",\n            \"analyzer\": \"snowball\"\n        }\n    }\n}\n"
        },
        "elasticsearchConfig": {
            "host": "https://readonly:onlyread@50896fdf5c15388f8976945e5582a856.eu-west-1.aws.found.io",
            "apiVersion": "5.6",
