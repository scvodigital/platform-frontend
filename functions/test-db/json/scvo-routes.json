{
    "index": {
        "linkTags": [],
        "metaTags": [],
        "pattern": "/",
        "template": "<section class=\"v-padding\">\n    <div class=\"container-fluid\">\n        <h1>Featured</h1>\n        {{#each primaryResponse.hits.hits}}\n            {{{_source.rendered.featured}}}   \n        {{/each}}\n    </div>\n</section>\n\n<section class=\"v-padding\">\n    <div class=\"container-fluid\">\n        <h2>Latest Blog Posts</h2>\n        <div class=\"mdc-layout-grid\">\n            <div class=\"mdc-layout-grid__inner\">\n                <div class=\"mdc-layout-grid__cell\">\n                    {{{supplimentaryResponse.blogs_latest.hits.hits.[0]._source.rendered.gist}}}\n                </div>\n                <div class=\"mdc-layout-grid__cell\">\n                    {{{supplimentaryResponse.blogs_latest.hits.hits.[1]._source.rendered.gist}}}\n                </div>\n                <div class=\"mdc-layout-grid__cell\">\n                    {{{supplimentaryResponse.blogs_latest.hits.hits.[2]._source.rendered.gist}}}\n                </div>\n            </div>\n        </div>\n        <h2>Latest Goodmoves Jobs</h2>\n        <div class=\"mdc-layout-grid\">\n            <div class=\"mdc-layout-grid__inner\">\n                <div class=\"mdc-layout-grid__cell\">\n                    {{{supplimentaryResponse.goodmoves_latest.hits.hits.[0]._source.rendered.gist}}}\n                </div>\n                <div class=\"mdc-layout-grid__cell\">\n                    {{{supplimentaryResponse.goodmoves_latest.hits.hits.[1]._source.rendered.gist}}}\n                </div>\n                <div class=\"mdc-layout-grid__cell\">\n                    {{{supplimentaryResponse.goodmoves_latest.hits.hits.[2]._source.rendered.gist}}}\n                </div>\n            </div>\n        </div>\n        <h2>Latest on TFN</h2>\n        <div class=\"mdc-layout-grid\">\n            <div class=\"mdc-layout-grid__inner\">\n                <div class=\"mdc-layout-grid__cell\">\n                    {{{supplimentaryResponse.tfn_latest.hits.hits.[0]._source.rendered.gist}}}\n                </div>\n                <div class=\"mdc-layout-grid__cell\">\n                    {{{supplimentaryResponse.tfn_latest.hits.hits.[1]._source.rendered.gist}}}\n                </div>\n                <div class=\"mdc-layout-grid__cell\">\n                    {{{supplimentaryResponse.tfn_latest.hits.hits.[2]._source.rendered.gist}}}\n                </div>\n            </div>\n        </div>\n    </div>\n</section>\n",
        "primarySearchTemplate": {
            "index": "web-content-production",
            "type": "scvo-cms-page,scvo-cms-post",
            "template": "{\n\t\"size\": 4,\n\t\"query\": {\n        \"bool\": {\n            \"must\": [\n                {\n    \t\t        \"term\": {\n                        \"featured\": true\n                    }\n                }\n            ]\n        }\n\t},\n\t\"sort\": {\n\t\t\"publish_on\": \"desc\"\n\t}\n}\n"
        },
        "supplimentarySearchTemplates": {
            "goodmoves_latest": {
                "index": "web-content-production",
                "type": "goodmoves-legacy",
                "template": "{\n    \"size\": 3,\n    \"_source\": [\"rendered.gist\"],\n    \"query\": {\n        \"bool\": {\n            \"must\": [\n                {\n                    \"term\": {\n                        \"country_iso3\": \"GBR\"\n                    }\n                },\n                {\n                    \"term\": {\n                        \"vacancy_status\": \"vacancy-open\"\n                    }\n                }\n            ],\n            \"should\": [\n                {\n                    \"term\": {\n                        \"top_job\": {\n                            \"value\": true,\n                            \"boost\": 120\n                        }\n                    }\n                }\n            ]\n        }\n    },\n    \"sort\": {\n        \"_score\": \"desc\"\n    }\n}\n"
            },
            "blogs_latest": {
                "index": "web-content-production",
                "type": "scvo-cms-post",
                "template": "{\n\t\"size\": 3,\n    \"_source\": [\"rendered.gist\"],\n\t\"query\": {\n        \"bool\": {\n            \"must_not\": [\n                {\n    \t\t        \"term\": {\n                        \"featured\": true\n                    }\n                }\n            ]\n        }\n\t},\n\t\"sort\": {\n\t\t\"publish_on\": \"desc\"\n\t}\n}\n"
            },
            "tfn_latest": {
                "index": "web-content-production",
                "type": "tfn-news-post",
                "template": "{\n\t\"size\": 3,\n    \"_source\": [\"rendered.gist\"],\n\t\"query\": {\n        \"match_all\": { }\n\t},\n\t\"sort\": {\n\t\t\"date_published\": \"desc\"\n\t}\n}\n"
            }
        },
        "elasticsearchConfig": {
            "host": "https://readonly:onlyread@50896fdf5c15388f8976945e5582a856.eu-west-1.aws.found.io",
            "apiVersion": "5.6",
            "log": "trace"
        }
    },
    "main-search": {
        "linkTags": [],
        "metaTags": [],
        "pattern": "/search",
        "template": "<div class=\"container-fluid\">\n    <div class=\"v-margin\">\n        <div class=\"mdc-card \">\n            <section class=\"mdc-card__primary\">\n                <h2 class=\"mdc-card__title mdc-card__title--large\">Search SCVO</h2>\n                <input type=\"text\" class=\"mdc-textfield__input searchRoute\" placeholder=\"Search\" \n                    [searchroute]='\"main-search\"' [paramstemplate]='\"{ \\\"queryParams\\\": { \\\"q\\\": %q } }\"'>\n            </section>\n        </div>\n    </div>\n\n    {{#each primaryResponse.hits.hits}}\n        <div class=\"v-margin\">\n            {{#if _source.rendered.search_result_scvo}}\n                {{{_source.rendered.search_result_scvo}}}\n            {{else}}\n                {{#if _source.rendered.search_result}}\n                    {{{_source.rendered.search_result}}}\n                {{/if}}\n            {{/if}}\n        </div>\n    {{/each}}\n</div>\n",
        "jsonLdTemplate": "",
        "primarySearchTemplate": {
            "index": "web-content-production",
            "type": "goodmoves-legacy,scvo-cms-page,scvo-cms-post,scvo-member,digital-charter-signatory,funding-scotland-fund,goodhq-organisation,scvo-document-evidence,scvo-event,scvo-grant-digital,scvo-staff",
            "template": "{\n    \"size\": 10,\n    \"query\": {\n        \"query_string\": {\n            \"query\": \"{{query.q}}\",\n            \"default_operator\": \"AND\",\n            \"analyzer\": \"snowball\"\n        }\n    }\n}\n"
        },
        "elasticsearchConfig": {
            "host": "https://readonly:onlyread@50896fdf5c15388f8976945e5582a856.eu-west-1.aws.found.io",
            "apiVersion": "5.6",
            "log": "trace"
        }
    },
    "post": {
        "linkTags": [],
        "metaTags": [],
        "pattern": "/blog/*path",
        "template": "{{#with supplimentaryResponse.splash.hits.hits.[0]._source}}\n<div class=\"splash-image\" style=\"background-image: linear-gradient(to bottom left,rgba(230, 74, 25, 0.29),#BF360C),url({{original_image}});position: relative;\">\n    <div class=\"container-fluid\">\n        <div class=\"mdc-layout-grid\">\n            <h1 class=\"post-title\">{{#with primaryResponse.hits.hits.[0]._source}}{{sanitize title}}{{/with}}</h1>\n        </div>\n    </div>\n</div>\n{{/with}}\n\n<div class=\"container-fluid\">\n    <div class=\"mdc-layout-grid\">\n        <div class=\"mdc-layout-grid__inner\">\n            <div class=\"mdc-layout-grid__cell mdc-layout-grid__cell--span-8\">\n                {{#with primaryResponse.hits.hits.[0]._source}}\n                {{{rendered.full}}}\n                {{/with}}\n            </div>\n            <div class=\"mdc-layout-grid__cell mdc-layout-grid__cell--span-4\">\n                <h3 class=\"\">More by {{primaryResponse.hits.hits.[0]._source.author_name}}</h3>\n                <ul class=\"supplimentary-list\">\n                    {{#each supplimentaryResponse.authors_posts.hits.hits}}\n                    {{{_source.rendered.list}}}\n                    {{/each}}\n                </ul>\n            </div>\n        </div>\n    </div>\n</div>\n",
        "primarySearchTemplate": {
            "index": "web-content-production",
            "type": "scvo-cms-post",
            "template": "{\n    \"size\": 1,\n    \"query\": {\n        \"term\": {\n            \"slug\": \"{{path}}\"\n        }\n    }\n}\n"
        },
        "supplimentarySearchTemplates": {
            "authors_posts": {
                "index": "web-content-production",
                "type": "scvo-cms-post",
                "template": "{\n    \"size\": 5,\n    \"query\": {\n        \"term\": {\n            \"author\": \"{{primaryResponse.hits.hits.[0]._source.author}}\"\n        }\n    }\n}\n"
            },
            "splash": {
                "index": "web-content-production",
                "type": "scvo-cms-media",
                "template": "{\n\t\"size\": 1,\n\t\"query\": {\n\t\t\"bool\": {\n\t\t\t\"should\": [\n\t\t\t\t{\n\t\t\t\t\t\"terms\": {\n\t\t\t\t\t\t\"categories_id\": [\n                            {{join primaryResponse.hits.hits.[0]._source.categories_id}}\n\t\t\t\t\t\t]\n\t\t\t\t\t}\n\t\t\t\t},\n\t\t\t\t{\n\t\t\t\t\t\"match_all\": { }\n\t\t\t\t}\n\t\t\t]\n\t\t}\n\t}\n}\n"
            }
        },
        "elasticsearchConfig": {
            "host": "https://readonly:onlyread@50896fdf5c15388f8976945e5582a856.eu-west-1.aws.found.io",
            "apiVersion": "5.6",
            "log": "trace"
        }
    },
    "_default": {
        "linkTags": [],
        "metaTags": [],
        "pattern": "/*path",
        "template": "{{#with primaryResponse.hits.hits.[0]._source}}\n<div class=\"container-fluid\">\n    {{#if breadcrumb}}\n    <ul class=\"breadcrumb\">\n        <li><a href=\"/\">Home</a></li>\n        {{#each breadcrumb}}\n        <li>\n            <a href=\"{{url}}\">{{{striptags title}}}</a>\n        </li>\n        {{/each}}\n    </ul>\n    {{/if}}\n    {{{rendered.full}}}\n</div>\n{{/with}}\n",
        "jsonLdTemplate": "{{#compare primaryResponse.hits.hits.length \">\" 0}}\n    {{#with primaryResponse.hits.hits.[0]._source}}\n        {{{rendered.json_ld}}}\n    {{/with}}\n{{else}}\n    { \"error\": \"404\" }\n{{/compare}}\n",
        "primarySearchTemplate": {
            "index": "web-content-production",
            "type": "scvo-cms-page",
            "template": "{\n    \"size\": 1,\n    \"query\": {\n        \"term\": {\n            \"slug\": \"{{path}}\"\n        }\n    }\n}\n"
        },
        "elasticsearchConfig": {
            "host": "https://readonly:onlyread@50896fdf5c15388f8976945e5582a856.eu-west-1.aws.found.io",
            "apiVersion": "5.6",
            "log": "trace"
        }
    }
