{
    "index": {
        "linkTags": [],
        "metaTags": [],
        "pattern": "/",
        "template": "<section class=\"v-padding\">\n    <div class=\"container-fluid\">\n        {{#each primaryResponse.hits.hits}}\n            {{{_source.rendered.featured}}}\n        {{/each}}\n    </div>\n</section>\n\n<section class=\"v-padding\">\n    <div class=\"container-fluid\">\n        <div class=\"latest-grid\">\n            <div class=\"mdc-layout-grid__inner\">\n                <div class=\"mdc-layout-grid__cell\">\n                    <h2>Latest blog posts</h2>\n                    {{{supplimentaryResponse.blogs_latest.hits.hits.[0]._source.rendered.gist}}}\n                    {{{supplimentaryResponse.blogs_latest.hits.hits.[1]._source.rendered.gist}}}\n                    {{{supplimentaryResponse.blogs_latest.hits.hits.[2]._source.rendered.gist}}}\n                </div>\n                <div class=\"mdc-layout-grid__cell\">\n                    <h2>Top <a href=\"http://goodmoves.com\"><strong>good</strong>moves</a> jobs</h2>\n                    {{{supplimentaryResponse.goodmoves_latest.hits.hits.[0]._source.rendered.gist}}}\n                    {{{supplimentaryResponse.goodmoves_latest.hits.hits.[1]._source.rendered.gist}}}\n                    {{{supplimentaryResponse.goodmoves_latest.hits.hits.[2]._source.rendered.gist}}}\n                </div>\n                <div class=\"mdc-layout-grid__cell\">\n                    <h2>Latest <a href=\"http://thirdforcenews.org.uk\">TFN</a> news</h2>\n                    {{{supplimentaryResponse.tfn_latest.hits.hits.[0]._source.rendered.gist}}}\n                    {{{supplimentaryResponse.tfn_latest.hits.hits.[1]._source.rendered.gist}}}\n                    {{{supplimentaryResponse.tfn_latest.hits.hits.[2]._source.rendered.gist}}}\n                </div>\n            </div>\n        </div>\n    </div>\n</section>\n",
        "primarySearchTemplate": {
            "index": "web-content-production",
            "type": "scvo-cms-page,scvo-cms-post",
            "template": "{\n\t\"size\": 4,\n\t\"query\": {\n        \"bool\": {\n            \"must\": [\n                {\n    \t\t        \"term\": {\n                        \"featured\": true\n                    }\n                }\n            ]\n        }\n\t},\n\t\"sort\": {\n\t\t\"publish_on\": \"desc\"\n\t}\n}\n"
        },
        "supplimentarySearchTemplates": {
            "goodmoves_latest": {
                "index": "web-content-production",
                "type": "goodmoves-legacy",
                "template": "{\n    \"size\": 3,\n    \"_source\": [\"rendered.gist\"],\n    \"query\": {\n        \"bool\": {\n            \"must\": [\n                {\n                    \"term\": {\n                        \"country_iso3\": \"GBR\"\n                    }\n                },\n                {\n                    \"term\": {\n                        \"vacancy_status\": \"vacancy-open\"\n                    }\n                }\n            ],\n            \"should\": [\n                {\n                    \"term\": {\n                        \"top_job\": {\n                            \"value\": true,\n                            \"boost\": 120\n                        }\n                    }\n                }\n            ]\n        }\n    },\n    \"sort\": {\n        \"_score\": \"desc\"\n    }\n}\n"
            },
            "blogs_latest": {
                "index": "web-content-production",
                "type": "scvo-cms-post",
                "template": "{\n\t\"size\": 3,\n    \"_source\": [\"rendered.gist\"],\n\t\"query\": {\n        \"bool\": {\n            \"must_not\": [\n                {\n    \t\t        \"term\": {\n                        \"featured\": true\n                    }\n                }\n            ]\n        }\n\t},\n\t\"sort\": {\n\t\t\"publish_on\": \"desc\"\n\t}\n}\n"
