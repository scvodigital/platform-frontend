{
    "main-search": {
        "linkTags": [],
        "metaTags": [],
        "pattern": "/search",
        "template": "<div class=\"container-fluid\">\n    <div class=\"mdc-card\">\n        <section class=\"mdc-card__primary\">\n            <h2 class=\"mdc-card__title mdc-card__title--large\">Search SCVO</h2>\n            <input type=\"text\" class=\"mdc-textfield__input searchRoute\" placeholder=\"Search\" \n                [searchroute]='\"main-search\"' [paramstemplate]='\"{ \\\"queryParams\\\": { \\\"q\\\": %q } }\"'>\n        </section>\n    </div>\n\n    {{#each primaryResponse.hits.hits}}\n        {{#if _source.rendered.search_result_scvo}}\n            {{{_source.rendered.search_result_scvo}}}\n        {{else}}\n            {{#if _source.rendered.search_result}}\n                {{{_source.rendered.search_result}}}\n            {{/if}}\n        {{/if}}\n    {{/each}}\n</div>\n",
        "jsonLdTemplate": "",
        "primarySearchTemplate": {
            "index": "web-content-production",
            "type": "goodmoves-legacy,scvo-cms-page,scvo-cms-post,digital-charter-signatory,funding-scotland-fund,goodhq-organisation,milo-volunteering-opportunity,scvo-document-evidence,scvo-event,scvo-grant-digital,scvo-staff,tfn-news-post,tfn-blog-post,tfn-event,tfn-poll",
            "template": "{\n    \"size\": 10,\n    \"query\": {\n        \"query_string\": {\n            \"query\": \"{{query.q}}\",\n            \"default_operator\": \"AND\",\n            \"analyzer\": \"snowball\"\n        }\n    }\n}\n"
        },
        "elasticsearchConfig": {
            "host": "https://readonly:onlyread@50896fdf5c15388f8976945e5582a856.eu-west-1.aws.found.io",
            "apiVersion": "5.5",
            "log": "trace"
        }
    },
    "post": {
        "linkTags": [],
        "metaTags": [],
        "pattern": "/blog/*path",
        "template": "<div class=\"container-fluid\">\n    <div class=\"mdc-layout-grid\">\n        <div class=\"mdc-layout-grid__inner\">\n            <div class=\"mdc-layout-grid__cell mdc-layout-grid__cell--span-8\">\n                {{#with primaryResponse.hits.hits.[0]._source}}\n                    {{{rendered.full}}}   \n                {{/with}}\n            </div>\n            <div class=\"mdc-layout-grid__cell mdc-layout-grid__cell--span-4\">\n                {{#with supplimentaryResponse.author.hits.hits.[0]._source}}\n                    {{{rendered.post}}}\n                {{/with}}\n                {{#each supplimentaryResponse.authors_posts.hits.hits}}\n                    {{{_source.rendered.search_result_scvo}}}\n                {{/each}}\n            </div>\n        </div>\n    </div>\n</div>\n",
        "primarySearchTemplate": {
            "index": "web-content-production",
            "type": "scvo-cms-post",
            "template": "{\n    \"size\": 1,\n    \"query\": {\n        \"term\": {\n            \"slug\": \"{{path}}\"\n        }\n    }\n}\n"
        },
        "supplimentarySearchTemplates": {
            "author": {
                "index": "web-content-production",
                "type": "scvo-cms-author",
                "template": "{\n    \"size\": 1,\n    \"query\": {\n        \"term\": {\n            \"wpid\": \"{{primaryResponse.hits.hits.[0]._source.author}}\"\n        }\n    }\n}\n"
            },
            "authors_posts": {
                "index": "web-content-production",
                "type": "scvo-cms-post",
                "template": "{\n    \"size\": 5,\n    \"query\": {\n        \"term\": {\n            \"author\": \"{{primaryResponse.hits.hits.[0]._source.author}}\"\n        }\n    }\n}\n"
            }
        },
        "elasticsearchConfig": {
            "host": "https://readonly:onlyread@50896fdf5c15388f8976945e5582a856.eu-west-1.aws.found.io",
            "apiVersion": "5.5",
            "log": "trace"
        }
    },
    "_default": {
        "linkTags": [],
        "metaTags": [],
        "pattern": "/",
        "template": "<div class=\"container-fluid\">\n    {{#with primaryResponse.hits.hits.[0]._source}}\n        {{{rendered.full}}}   \n    {{/with}}\n</div>\n",
        "jsonLdTemplate": "{{#compare primaryResponse.hits.hits.length \">\" 0}}\n    {{#with primaryResponse.hits.hits.[0]._source}}\n        {{{rendered.json_ld}}}\n    {{/with}}\n{{else}}\n    { \"error\": \"404\" }\n{{/compare}}\n",
        "primarySearchTemplate": {
            "index": "web-content-production",
            "type": "scvo-cms-page",
            "template": "{\n    \"size\": 1,\n    \"query\": {\n        \"term\": {\n            \"slug\": \"{{path}}\"\n        }\n    }\n}\n"
        },
        "elasticsearchConfig": {
            "host": "https://readonly:onlyread@50896fdf5c15388f8976945e5582a856.eu-west-1.aws.found.io",
            "apiVersion": "5.5",
            "log": "trace"
        }
    }
}