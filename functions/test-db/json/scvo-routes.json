{
    "_default": {
        "linkTags": [],
        "metaTags": [],
        "pattern": "/",
        "template": "<div class=\"container-fluid\">\n    {{#with primaryResponse.hits.hits.[0]._source}}\n        {{{rendered.full}}}   \n    {{/with}}\n</div>\n",
        "jsonLdTemplate": "{{#compare primaryResponse.hits.hits.length \">\" 0}}\n    {{#with primaryResponse.hits.hits.[0]._source}}\n        {{{rendered.json_ld}}}\n    {{/with}}\n{{else}}\n    { \"error\": \"404\" }\n{{/compare}}\n",
        "primarySearchTemplate": {
            "index": "web-content-production",
            "type": "scvo-cms-page",
            "template": "{\n    \"size\": 1,\n    \"query\": {\n        \"term\": {\n            \"slug\": \"{{path}}\"\n        }\n    }\n}\n"
        },
        "elasticsearchConfig": {
            "host": "https://readonly:onlyread@50896fdf5c15388f8976945e5582a856.eu-west-1.aws.found.io",
            "apiVersion": "5.5",
            "log": "trace"
        }
    },
    "main-search": {
        "linkTags": [],
        "metaTags": [],
        "pattern": "/search",
        "template": "<div class=\"container-fluid\">\n    {{#each primaryResponse.hits.hits}}\n        {{#if _source.rendered.search_result_scvo}}\n            {{{_source.rendered.search_result_scvo}}}\n        {{else}}\n            {{#if _source.rendered.search_result}}\n                {{{_source.rendered.search_result}}}\n            {{/if}}\n        {{/if}}\n    {{/each}}\n</div>\n",
        "jsonLdTemplate": "",
        "primarySearchTemplate": {
            "index": "web-content-production",
            "type": "goodmoves-job,scvo-cms-page,scvo-cms-post",
            "template": "{\n    \"size\": 10,\n    \"query\": {\n        \"query_string\": {\n            \"query\": \"{{query.q}}\",\n            \"default_operator\": \"AND\",\n            \"analyzer\": \"snowball\"\n        }\n    }\n}\n"
        },
        "elasticsearchConfig": {
            "host": "https://readonly:onlyread@50896fdf5c15388f8976945e5582a856.eu-west-1.aws.found.io",
            "apiVersion": "5.5",
            "log": "trace"
        }
    },
    "posts": {
        "linkTags": [],
        "metaTags": [],
        "pattern": "/",
        "template": "<div class=\"container-fluid\">\n    {{#with primaryResponse.hits.hits.[0]._source}}\n        {{{rendered.full}}}   \n    {{/with}}\n</div>\n",
        "primarySearchTemplate": {
            "type": "goodmoves-job",
            "template": "{\n    \"size\": 1,\n    \"query\": {\n        \"term\": {\n            \"slug\": \"{{path}}\"\n        }\n    }\n}\n"
        },
        "elasticsearchConfig": {
            "host": "https://readonly:onlyread@50896fdf5c15388f8976945e5582a856.eu-west-1.aws.found.io",
            "apiVersion": "5.5"
        }
    }
}