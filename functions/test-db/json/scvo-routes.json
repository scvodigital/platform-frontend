        "primarySearchTemplate": {
            "index": "web-content-production",
            "type": "scvo-event",
            "template": "{\n    \"size\": 1,\n    \"query\": {\n        \"term\": {\n            \"slug\": \"{{split path '_'}}\"\n        }\n    }\n}\n"
        },
