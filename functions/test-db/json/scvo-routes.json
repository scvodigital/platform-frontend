            "types": {
                "index": "web-content-production",
                "type": "scvo-event",
                "template": "{\n    \"size\": 0,\n    \"aggs\": {\n        \"types\": {\n            \"terms\": {\n                \"field\": \"type\",\n                \"size\": 10000\n            }\n        }\n    }\n}\n"
