        "title": "Goodmoves - {{title}}",
        "template": "{{#and primaryResultSet primaryResultSet.documents}}\n    {{#with primaryResultSet.documents.[0]}}\n    <section class=\"hero\">\n        <div class=\"container-fluid\">\n            <div class=\"mdc-card\">\n                <section class=\"mdc-card__primary mdc-theme--primary-bg\">\n                    <h2 class=\"mdc-card__title mdc-card__title--large\">{{title}}</h2>\n                    <h3 class=\"mdc-card__subtitle\">\n                        Posted By: \n                        <a href=\"/organisation/{{organisation.slug}}\">\n                            <strong>{{organisation.name}}</strong>\n                            {{#if organisation.charity_number}}\n                                <small>[{{organisation.charity_number}}]</small>\n                            {{/if}}\n                        </a>\n                    </h3>\n                </section>\n                <div class=\"mdc-card__horizontal-block\">\n                    <section class=\"mdc-card__supplimentary-text\">\n                        <ul class=\"mdc-list mdc-list--two-line mdc-list--dense\">\n                            <li class=\"mdc-list-item\">\n                                <span class=\"mdc-list-item__text\">\n                                    Salary:\n                                    <span class=\"mdc-list-item__text__secondary\">\n                                        {{#eq remuneration_type \"Salaried\"}}\n                                            {{#and (not salary_min) (not salary_max)}}\n                                                TBC\n                                            {{else}}\n                                                {{#if salary_circa}} Circa {{/if}} \n                                                {{#and (gt salary_min 0) (not salary_max)}}\n                                                    from &pound;{{addCommas salary_min}}\n                                                {{else}}\n                                                    {{#and (not salary_min) (gt salary_max 0)}}\n                                                        up to &pound;{{addCommas salary_max}}\n                                                    {{else}}\n                                                        {{#gt salary_max salary_min}}\n                                                            &pound;{{addCommas salary_min}} - &pound;{{addCommas salary_max}}\n                                                        {{else}}\n                                                            &pound;{{addCommas salary_min}}\n                                                        {{/gt}}\n                                                    {{/and}}\n                                                {{/and}}\n                                            {{/and}}\n                                            {{#eq status \"Part time\"}} pro-rata {{/eq}}\n                                        {{else}}\n                                            {{#if remuneration_type}} {{remuneration_type}} {{/if}} \n                                        {{/eq}}\n                                    </span>\n                                </span>\n                            </li>\n                            \n                            <li class=\"mdc-list-item\">\n                                <span class=\"mdc-list-item__text\">\n                                    Salary info:\n                                    <span class=\"mdc-list-item__text__secondary\">\n                                        {{salary_info}}\n                                    </span>\n                                </span>\n                            </li>\n                            \n                            <li class=\"mdc-list-item\">\n                                <span class=\"mdc-list-item__text\">\n                                    Closing date:\n                                    <span class=\"mdc-list-item__text__secondary\">\n                                        {{moment closing_date \"D MMMM YYYY\"}}\n                                    </span>\n                                </span>\n                            </li>\n                            \n                            <li class=\"mdc-list-item\">\n                                <span class=\"mdc-list-item__text\">\n                                    Location:\n                                    <span class=\"mdc-list-item__text__secondary\">\n                                        {{location}}\n                                    </span>\n                                </span>\n                            </li>\n\n                            {{#if organisation.website}}\n                                <li class=\"mdc-list-item\">\n                                    <span class=\"mdc-list-item__text\">\n                                        Website:\n                                        <span class=\"mdc-list-item__text__secondary\">\n                                            <a href=\"{{organisation.website}}\">\n                                                {{organisation.website}}\n                                            </a>\n                                        </span>\n                                    </span>\n                                </li>\n                            {{/if}} \n                            {{#if organisation.twitter}}\n                                <li class=\"mdc-list-item\">\n                                    <span class=\"mdc-list-item__text\">\n                                        Twitter\n                                        <span class=\"mdc-list-item__text__secondary\">\n                                            <a href=\"{{organisation.twitter}}\">\n                                                {{organisation.twitter}}\n                                            </a>\n                                        </span>\n                                    </span>\n                                </li>\n                            {{/if}} \n                            {{#if organisation.facebook}}\n                                <li class=\"mdc-list-item\">\n                                    <span class=\"mdc-list-item__text\">\n                                        Facebook\n                                        <span class=\"mdc-list-item__text__secondary\">\n                                            <a href=\"{{organisation.facebook}}\">\n                                                {{organisation.facebook}}\n                                            </a>\n                                        </span>\n                                    </span>\n                                </li>\n                            {{/if}} \n                        </ul>\n                    </section>\n                    <a href=\"/organisation/{{organisation.slug}}\">\n                        {{#if organisation.has_logo}}\n                            <img src=\"{{organisation.logo_url}}\" class=\"mdc-card__media-item mdc-card__media-item--2x\">\n                        {{else}}\n                            <img src=\"https://goodmoves.org.uk/app/assets/images/no-logo.png\" class=\"mdc-card__media-item mdc-card__media-item--2x\">\n                        {{/if}}\n                    </a>\n                </div>\n            </div>\n        </div>\n    </section>\n\n    <div class=\"container-fluid\">\n        <div class=\"mdc-card\">\n            <section class=\"mdc-card__supporting-text\">\n                {{#if description}}\n                    <h4>Role</h4>\n                    <div class=\"details-markdown\">\n                        {{#markdown}}{{{description}}}{{/markdown}}\n                    </div>\n                    <hr /> \n                {{/if}}\n                {{#if organisation_profile}}\n                    <h4>Organisation profile</h4>\n                    <div class=\"details-markdown\">\n                        {{#markdown}}{{{organisation.profile}}}{{/markdown}}\n                    </div>\n                    <hr /> \n                {{/if}}\n                {{#if application_notes}}\n                    <h4>Application notes</h4>\n                    <div class=\"details-markdown\">\n                        {{#markdown}}{{{application_notes}}}{{/markdown}}\n                    </div>\n                    <hr /> \n                {{/if}} \n                {{#if assets}}\n                    <h4>Files</h4>\n                    <div class=\"collection\"> \n                        {{#forEach assets}}\n                            <a href=\"https://goodmoves.blob.core.windows.net/vacancyassets/{{id}}-{{filename}}\" class=\"collection-item avatar\"> \n                                <i class=\"fa fa-3x fa-file-o circle green white-text\"></i>\n                                <span class=\"title\"> {{titleize title}} </span>\n                            </a> \n                        {{/forEach}} \n                    </div> \n                {{/if}} \n            </section>\n        </div>            \n    </div>\n    {{/with}}\n{{/and}}\n\n",
