{{#with primaryResponse.hits.hits.[0]._source}}
<div class="container-fluid">
    {{#if breadcrumb}}
    {{#compare breadcrumb.length '>=' 1}}
    <ul class="breadcrumb">
        <li><a href="/">Home</a></li>
        {{#each breadcrumb}}
        <li>
            <a href="{{url}}">{{{striptags title}}}</a>
        </li>
        {{/each}}
    </ul>
    {{/compare}}
    {{/if}}
    {{{rendered.full}}}
</div>
{{/with}}
