<div class="page jobs-landing">
    {{> header_splash}}
    <div class="container-fluid">
        {{#> handle_404}}
        {{> breadcrumbs}}
        <div class="mdc-layout-grid">
            <div class="mdc-layout-grid__inner">
                <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-12 content page-content">
                    <div class="mdc-layout-grid">
                        <div class="mdc-layout-grid__inner">
                            <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-3-desktop mdc-layout-grid__cell--span-8-tablet  mdc-layout-grid__cell--span-4-phone title">
                                <a href="https://goodmoves.com"><img src="https://storage.googleapis.com/scvo-cms/scvo-cms-content/images/banners/goodmoves-side.png" alt="Goodmoves" /></a>
                            </div>
                            <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-5-desktop mdc-layout-grid__cell--span-4-tablet  mdc-layout-grid__cell--span-4-phone search">
                                <h2>Find a job in the third sector</h2>
                                <form action="/jobs/search" method="GET" [searchroute]="'goodmoves_job_search'" class="search_form" data-auto-search="true">
                                    <div class="mdc-textfield mdc-textfield--upgraded">
                                        <input type="text" class="mdc-textfield__input" name="q" id="jobs-keywords" placeholder="Keywords" value="{{params.query.q}}">
                                        <button type="submit" class="mdc-toolbar__icon mdc-toolbar__search search-button-large" aria-label="Search" alt="Search">
                                            <span class="far fa-2x fa-search" aria-hidden="true"></span>
                                        </button>
                                    </div>
                                </form>
                                <p>
                                    Try: <a href="/jobs/search?region=East+of+Scotland">East of Scotland</a>, <a href="/jobs/search?role=Management">Management</a>, <a href="/jobs/search?sector=Social+Care">Social Care</a>
                                </p>
                                <p>
                                    <a href="https://goodmoves.com/recruiting">Advertise with us</a>
                                </p>
                            </div>
                            <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-4-desktop mdc-layout-grid__cell--span-4-tablet  mdc-layout-grid__cell--span-4-phone top-jobs">
                                {{#each supplimentaryResponses.goodmoves_latest.hits.hits}}
                                {{{_source.rendered.gist}}}
                                {{/each}}
                            </div>
                        </div>
                        <div class="mdc-layout-grid__inner">
                            <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-3-desktop mdc-layout-grid__cell--span-8-tablet  mdc-layout-grid__cell--span-4-phone title">
                                <a href="/jobs/community-jobs-scotland"><img src="https://storage.googleapis.com/scvo-cms/scvo-cms-content/images/banners/cjs-side.png" alt="Community Jobs Scotland" /></a>
                            </div>
                            <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-5-desktop mdc-layout-grid__cell--span-4-tablet  mdc-layout-grid__cell--span-4-phone search">
                                <h2>Creating jobs in Scotland's third sector for unemployed young people</h2>
                                <p>
                                    Community Jobs Scotland has worked with hundreds of third sector organisations to support over 8,000 unemployed young people into work.
                                </p>
                                <ul>
                                    <li>Third sector employer? <a href="/jobs/community-jobs-scotland/information-for-employers">Find out how to get involved</a></li>
                                    <li>Aged 16-19? <a href="/jobs/community-jobs-scotland/information-for-employees">Find out hot to get employed through CJS</a></li>
                                    <li>SDS or JCP employee? <a href="/jobs/community-jobs-scotland/vacancies">Access vacancy information</a></li>
                                </ul>
                            </div>
                            <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-4-desktop mdc-layout-grid__cell--span-4-tablet  mdc-layout-grid__cell--span-4-phone top-jobs">
                                {{#each supplimentaryResponses.cjs_latest.hits.hits}}
                                {{{_source.rendered.scvo_gist}}}
                                {{/each}}
                            </div>
                        </div>
                    </div>
                </div>
                {{#compare supplimentaryResponses.related_events.hits.total ">" 3}}
                <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-12 related page-related">
                    <h3 class="blogs">Our training courses</h3>
                    <div class="mdc-layout-grid">
                        <div class="mdc-layout-grid__inner">
                            <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-3-desktop mdc-layout-grid__cell--span-4-tablet  mdc-layout-grid__cell--span-4-phone">
                                {{{supplimentaryResponses.related_training.hits.hits.[0]._source.rendered.scvo_gist}}}
                            </div>
                            <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-3-desktop mdc-layout-grid__cell--span-4-tablet  mdc-layout-grid__cell--span-4-phone">
                                {{{supplimentaryResponses.related_training.hits.hits.[1]._source.rendered.scvo_gist}}}
                            </div>
                            <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-3-desktop mdc-layout-grid__cell--span-4-tablet  mdc-layout-grid__cell--span-4-phone">
                                {{{supplimentaryResponses.related_training.hits.hits.[2]._source.rendered.scvo_gist}}}
                            </div>
                            <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-3-desktop mdc-layout-grid__cell--span-4-tablet  mdc-layout-grid__cell--span-4-phone">
                                {{{supplimentaryResponses.related_training.hits.hits.[3]._source.rendered.scvo_gist}}}
                            </div>
                        </div>
                    </div>
                </div>
                {{/compare}}
            </div>
        </div>
        {{/handle_404}}
    </div>
</div>