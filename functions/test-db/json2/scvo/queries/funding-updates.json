{
	"size": 200,
    "_source": [
        "title",
        "rendered.scvo_list"
    ],
	"sort": {
		"lastImportantActivity": "desc"
	},
	"query": {
	    "bool": {
	        "must": [
	            {
	                "range": {
	                    "lastImportantActivity": {
	                        "gte": "{{firstItem (split (atob range) '-')}}",
	                        "lte": "{{lastItem (split (atob range) '-')}}"
	                    }
	                }
	            },
	            {
	                "terms": {
	                    "fundStatus": [ "Open" ]
	                }
	            }
	        ]
	    }
	}
}
