{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "cleanup": "rm -rf ./dist",
    "start": "npm run build;firebase serve --only functions",
    "build": "npm run cleanup; npm run json2;tsc",
    "json2": "node json2 ./test-db/json2/**/*.json2 ./test-db/json; node json2 ./test-db/db.json2 ./test-db; npm run scss",
    "deploy-db": "npm run json2; firebase database:set -y / ./test-db/db.json",
    "scss": "node-sass --output-style compressed test-db/json2/scvo/scvo-styles.scss > ../src/assets/css/scvo-styles.css;node-sass --output-style compressed test-db/json2/scvo/scvo-styles.scss > ../dist/assets/css/scvo-styles.css"
  },
  "dependencies": {
    "@types/jsdom": "^11.0.2",
    "@types/node-sass": "^3.10.32",
    "@types/q": "^1.0.5",
    "@types/request": "^2.0.6",
    "@types/uuid": "^3.4.3",
    "clayhandlebars": "^3.2.2",
    "compression": "^1.7.1",
    "cookie-parser": "^1.4.3",
    "dot-object": "^1.7.0",
    "firebase-admin": "5.4.0",
    "firebase-functions": "^0.7.0",
    "handlebars": "^4.0.10",
    "jsdom": "^11.3.0",
    "moment": "^2.18.1",
    "node-sass": "^4.5.3",
    "pdfmake": "^0.1.33",
    "q": "^1.5.0",
    "request": "^2.83.0",
    "scvo-router": "1.0.52",
    "string": "^3.3.3",
    "uuid": "^3.1.0"
  },
  "private": true,
  "main": "dist/index.js",
  "devDependencies": {
    "chalk": "^2.1.0",
    "dot-object": "^1.7.0",
    "typescript": "^2.5.3"
  }
}
