{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "cleanup": "rm -rf ./dist",
    "start": "npm run build;firebase serve --only functions",
    "build": "npm run router;npm run cleanup;npm run deploy-db;tsc",
    "json2": "node json2 ./test-db/**/*.json2 ./test-db/json; node json2 ./test-db/db.json2 ./test-db",
    "deploy-db": "npm run json2; firebase database:set -y / ./test-db/db.json"
  },
  "dependencies": {
    "@types/jsdom": "^11.0.2",
    "@types/node-sass": "^3.10.32",
    "firebase-admin": "~5.4.0",
    "firebase-functions": "^0.7.0",
    "handlebars": "^4.0.10",
    "handlebars-helpers": "^0.9.8",
    "jsdom": "^11.3.0",
    "moment": "^2.18.1",
    "node-sass": "^4.5.3",
    "pdfmake": "^0.1.33",
    "request": "^2.83.0",
    "scvo-router": "git+https://github.com/scvodigital/scvo-router.git#dev",
    "string": "^3.3.3"
  },
  "private": true,
  "main": "dist/index.js",
  "devDependencies": {
    "chalk": "^2.1.0",
    "dot-object": "^1.7.0"
  }
}
