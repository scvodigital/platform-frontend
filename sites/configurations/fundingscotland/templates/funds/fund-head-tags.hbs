{{#if data.primaryResponse.hits.hits.[0] ~}}
{{#with data.primaryResponse.hits.hits.[0]._source ~}}
<title>Funding Scotland – Fund from {{funder_organisation.name}} – {{title}}</title>
<meta property="og:title" content="Funding Scotland – Fund from {{funder_organisation.name}} – {{title}}" data-dynamic="true">
{{#ifAny og_image ../og_image ~}}
<meta property="og:image" content="{{default og_image ../og_image}}" data-dynamic="true">
{{/ifAny ~}}
{{#ifAny og_summary description ../og_summary ../description ~}}
<meta property="og:summary" content="{{stripTags (regexReplace (default og_summary (default ../og_summary (default description ../description))) '\r|\n' 'gi' '')}}" data-dynamic="true">
<meta name="description" content="{{stripTags (regexReplace (default description (default ../description (default og_summary ../og_summary))) '\r|\n' 'gi' '')}}" data-dynamic="true">
{{/ifAny ~}}
{{#if rendered.json_ld ~}}
<script type="application/ld+json" data-dynamic="true">
{{{rendered.json_ld}}}
</script>
{{/if ~}}
{{/with ~}}
{{else}}
<title>Funding Scotland – Fund</title>
<meta property="og:title" content="Funding Scotland – Fund" data-dynamic="true">
{{/if ~}}