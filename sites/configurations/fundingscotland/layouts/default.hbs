<link rel="stylesheet" href="https://pro.fontawesome.com/releases/v5.2.0/css/all.css" integrity="sha384-TXfwrfuHVznxCssTxWoPZjhcss/hp38gEOH8UPZG/JcXonvBQ6SlsIF49wUzsGno" crossorigin="anonymous">
{{#compare @root.request.url.hostname "fundingscotland.local" ~}}
  <link type="text/css" rel="stylesheet" href="/assets/fundingscotland/main.css">
{{else}}
  <link type="text/css" rel="stylesheet" href="https://storage.googleapis.com/scvo-assets/fundingscotland/main-{{@root.context.metaData.assetsVersion}}.css">
{{/compare ~}}
</head>
<body class="mdc-theme--site-bg {{#if request.params.path}}{{replace request.params.path '_' ' '}}{{else}}home{{/if}}">
  <fundingscotland-site>
    <div class="mdc-typography">
      {{#ifAny request.params.query.debug (compare request.url.hostname "goodmoves.local") ~}}
        {{#if errors.[0]}}
          <div style="z-index:100;position:fixed;top:0;right:0;padding:5px;background-color:#ff0000;color:#ffffff;">
            Route Task Errors (F12)
          </div>
        {{/if}}
      {{/ifAny}}
      <header class="mdc-top-app-bar mdc-elevation--z5 top-bar-stuck" id="top-bar">
        <div class="container-fluid">
          <div class="mdc-top-app-bar__row mdc-top-app-bar__upper">
            <section class="mdc-top-app-bar__section mdc-top-app-bar__section--align-start">
              <button class="mdc-top-app-bar__navigation-icon hide-lg" data-drawer-target="#mobile-menu">
                <span class="far fa-fw fa-2x fa-bars"></span>
              </button>
              {{#if @root.route.metaData.sidebarButton}}
              <button class="mdc-top-app-bar__navigation-icon {{@root.route.metaData.sidebarButton.classes}}" data-drawer-target="#sidebar-temporary" title="{{@root.route.metaData.sidebarButton.tooltip}}">
                <span class="far fa-fw fa-{{@root.route.metaData.sidebarButton.icon}}"></span>
              </button>
              {{/if}}
              <span class="mdc-top-app-bar__title hide-sm">
                <a href="/">
                  <span class="brand-text">Funding <span class="scotland">Scotland</span></span>
                </a>
              </span>
              <span class="mdc-top-app-bar__title hide-md hide-lg">
                <div class="brand-text">
                  Funding <span class="scotland">Scotland</span>
                </div>
              </span>
            </section>
            <section class="mdc-top-app-bar__section mdc-top-app-bar__section--align-end">
              <nav class="mdc-tab-bar mdc-tab-bar__right hide-sm hide-md" id="upper-tab-bar">
                {{#each @root.context.metaData.menus.upper ~}}
                <a class="mdc-tab{{#if (regexMatch @root.request.url.pathname route "gi")}} mdc-tab--active{{/if}}" href="{{stripTrailingSlash path}}">
                  <span class="mdc-tab--label">{{{label}}}</span>
                </a>
                {{/each ~}}
              </nav>
            </section>
          </div>
          <div class="mdc-top-app-bar__row mdc-top-app-bar__lower hide-sm hide-md">
            <section class="mdc-top-app-bar__section mdc-top-app-bar__section--align-start mdc-top-app-bar__main">
              <nav class="mdc-tab-bar mdc-tab-bar__left" id="main-tab-bar">
                {{#each @root.context.metaData.menus.main ~}}
                <a class="mdc-tab{{#if (regexMatch @root.request.url.pathname route "gi")}} mdc-tab--active{{/if}}" href="{{stripTrailingSlash path}}">
                  <span class="mdc-tab--label">{{{label}}}</span>
                </a>
                {{/each ~}}
              </nav>
              <nav class="mdc-tab-bar mdc-tab-bar__left" id="main-extra-tab-bar">
                {{#each @root.context.metaData.menus.main-extra ~}}
                <a class="mdc-tab{{#if (regexMatch @root.request.url.pathname route "gi")}} mdc-tab--active{{/if}}" href="{{stripTrailingSlash path}}">
                  <span class="mdc-tab--label">{{{label}}}</span>
                </a>
                {{/each ~}}
              </nav>
            </section>
            <section class="mdc-top-app-bar__section mdc-top-app-bar__section--align-end mdc-top-app-bar__user">
              {{#if @root.data.auth ~}}
              <div class="mdc-menu-anchor">
                <button class="mdc-button" data-menu-target="#user-menu">
                  {{#if @root.data.auth.photoURL ~}}
                  <img src="{{data.auth.photoURL}}" class="far fa-fw user-avatar" />
                  {{else}}
                  <span class="far fa-fw fa-user-circle"></span>
                  {{/if ~}}
                  {{{data.auth.email}}}
                  <span class="far fa-fw fa-angle-down"></span>
                </button>
                <div class="mdc-menu" tabindex="-1" data-mdc-auto-init="MDCMenu" id="user-menu">
                  <div class="mdc-menu__items mdc-list" role="menu" aria-hidden="true">
                    {{#each @root.context.metaData.menus.user-signed-in ~}}
                    <a class="mdc-list-item {{#if (regexMatch @root.request.url.pathname route "gi")}} mdc-list-item--activated{{/if}}" href="{{stripTrailingSlash path}}" role="menuitem">
                      {{#if icon ~}}
                      <span class="far fa-{{icon}} mdc-list-item__graphic" aria-hidden="true"></span>
                      {{/if ~}}
                      {{{label}}}
                    </a>
                    {{/each ~}}
                  </div>
                </div>
              </div>
              {{else}}
              <nav class="mdc-tab-bar mdc-tab-bar__right" id="user-not-signed-in-tab-bar">
                {{#each @root.context.metaData.menus.user-not-signed-in ~}}
                <a class="mdc-tab{{#if (regexMatch @root.request.url.pathname route "gi")}} mdc-tab--active{{/if}}" href="{{stripTrailingSlash path}}">
                  <span class="mdc-tab--label">{{{label}}}</span>
                </a>
                {{/each ~}}
              </nav>
              {{/if ~}}
            </section>
          </div>
        </div>
      </header>

      <aside class="mdc-drawer mdc-drawer--temporary" id="mobile-menu" data-mdc-auto-init="MDCTemporaryDrawer">
        <nav class="mdc-drawer__drawer">
          <div class="mdc-drawer__header">
            <div class="mdc-drawer__header-content mdc-top-app-bar__title">
              <div class="brand-text">
                Funding <span class="scotland">Scotland</span>
              </div>
            </div>
          </div>
          <nav class="mdc-drawer__content mdc-list">
            {{#each @root.context.metaData.menus.main ~}}
            <a class="mdc-list-item {{#if (regexMatch @root.request.url.pathname route "gi")}} mdc-list-item--activated{{/if}}" href="{{stripTrailingSlash path}}">
              {{#if icon ~}}
              <span class="far fa-{{icon}} mdc-list-item__graphic" aria-hidden="true"></span>
              {{/if ~}}
              {{{label}}}
            </a>
            {{/each ~}}
            {{#each @root.context.metaData.menus.main-extra ~}}
            <a class="mdc-list-item{{#if (regexMatch @root.request.url.pathname route "gi")}} mdc-list-item--activated{{/if}}" href="{{stripTrailingSlash path}}">
              {{#if icon ~}}
              <span class="far fa-{{icon}} mdc-list-item__graphic" aria-hidden="true"></span>
              {{/if ~}}
              {{{label}}}
            </a>
            {{/each ~}}
            {{#each @root.context.metaData.menus.upper ~}}
            <a class="mdc-list-item{{#if (regexMatch @root.request.url.pathname route "gi")}} mdc-list-item--activated{{/if}}" href="{{stripTrailingSlash path}}">
              {{#if icon ~}}
              <span class="far fa-{{icon}} mdc-list-item__graphic" aria-hidden="true"></span>
              {{/if ~}}
              {{{label}}}
            </a>
            {{/each ~}}
            <div role="separator" class="mdc-list-divider"></div>
            {{#if @root.data.auth ~}}
            {{#each @root.context.metaData.menus.user-signed-in ~}}
            <a class="mdc-list-item{{#if (regexMatch @root.request.url.pathname route "gi")}} mdc-list-item--activated{{/if}}" href="{{stripTrailingSlash path}}">
              {{#if icon ~}}
              <span class="far fa-{{icon}} mdc-list-item__graphic" aria-hidden="true"></span>
              {{/if ~}}
              {{{label}}}
            </a>
            {{/each ~}}
            {{else}}
            {{#each @root.context.metaData.menus.user-not-signed-in ~}}
            <a class="mdc-list-item{{#if (regexMatch @root.request.url.pathname route "gi")}} mdc-list-item--activated{{/if}}" href="{{stripTrailingSlash path}}">
              {{#if icon ~}}
              <span class="far fa-{{icon}} mdc-list-item__graphic" aria-hidden="true"></span>
              {{/if ~}}
              {{{label}}}
            </a>
            {{/each ~}}
            {{/if ~}}
          </nav>
        </nav>
      </aside>

      <main class="mdc-theme--background mdc-toolbar-fixed-adjust">
        {{#if layoutParts.header ~}}
          {{{layoutParts.header}}}
        {{/if ~}}
        <div class="{{@root.route.name}} {{#if layoutParts.sidebar}}drawer-container-permanent{{else}}drawer-container-temporary{{/if}}">
          {{#if layoutParts.sidebar ~}}
            <aside class="mdc-drawer mdc-drawer--temporary mdc-drawer--occasional mdc-elevation--z3" id="sidebar-temporary" data-mdc-auto-init="MDCTemporaryDrawer">
              <div class="mdc-drawer__drawer">
                <div class="mdc-drawer__header hide-lg">
                  <div class="mdc-drawer__header-content mdc-top-app-bar__title">
                    <div class="brand-text">
                      Funding <span class="scotland">Scotland</span>
                    </div>
                  </div>
                </div>
                {{{layoutParts.sidebar}}}
              </div>
            </aside>
          {{/if ~}}
          {{{layoutParts.body}}}
        </div>
      </main>

      <footer class="mdc-layout__footer">
        <div class="footer-main">
          <div class="container-fluid">
            <div class="mdc-layout-grid">
              <div class="mdc-layout-grid__inner">
                <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-3-desktop mdc-layout-grid__cell--span-4-tablet mdc-layout-grid__cell--span-4-phone contact">
                  <h3>Contact</h3>
                  <ul class="fa-ul">
                    <li>
                      <span class="fa-li"><span class="fas fa-envelope"></span></span>
                      <a href="mailto:fundingscotland@scvo.org.uk">fundingscotland@scvo.org.uk</a>
                    </li>
                    <li>
                      <span class="fa-li"><span class="fas fa-phone"></span></span>
                      <a href="tel:+448001690022">0800 169 0022</a>
                    </li>
                    <li>
                      <span class="fa-li"><span class="fab fa-twitter"></span></span>
                      <a href="https://twitter.com/fundingscotland" target="_blank">Twitter</a>
                    </li>
                  </ul>
                </div>
                <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-3-desktop mdc-layout-grid__cell--span-4-tablet mdc-layout-grid__cell--span-4-phone help">
                  <h3>Help</h3>
                  <ul class="mdc-list">
                    {{#each @root.context.metaData.menus.footer-help ~}}
                    <li class="mdc-list-item">
                      <a href="{{path}}">{{{label}}}</a>
                    </li>
                    {{/each ~}}
                  </ul>
                </div>
                <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-3-desktop mdc-layout-grid__cell--span-4-tablet mdc-layout-grid__cell--span-4-phone info">
                  <h3>Information</h3>
                  <ul class="mdc-list">
                    {{#each @root.context.metaData.menus.footer-info ~}}
                    <li class="mdc-list-item">
                      <a href="{{path}}">{{{label}}}</a>
                    </li>
                    {{/each ~}}
                  </ul>
                </div>
                <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-3-desktop mdc-layout-grid__cell--span-4-tablet mdc-layout-grid__cell--span-4-phone about">
                  <h3>About</h3>
                  <p>
                    <strong>Funding Scotland</strong> lists funders with a track record of supporting projects in Scotland and is run by the <a href="https://scvo.org">Scottish Council for Voluntary Organisations</a>. <a href="/about">Find out more</a>
                  </p>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="footer-copyright">
          <div class="container-fluid">
            <div class="mdc-layout-grid">
              <div class="mdc-layout-grid__inner">
                <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-10-desktop mdc-layout-grid__cell--span-6-tablet mdc-layout-grid__cell--span-4-phone">
                  <p>
                    &copy; {{momentFormat (moment) "YYYY"}}. The Scottish Council for Voluntary Organisations (SCVO) is a Scottish Charitable Incorporated Organisation.<br />
                    Charity registered in Scotland <a href="https://www.oscr.org.uk/about-charities/search-the-register/charity-details?number=SC003558">SC003558</a>. Registered office Mansfield Traquair Centre, 15 Mansfield Place, Edinburgh EH3 6BB.
                  </p>
                </div>
                <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-2-desktop mdc-layout-grid__cell--span-2-tablet mdc-layout-grid__cell--span-4-phone">
                  <a href="https://scvo.org" class="brand-logo" title="Scottish Council for Voluntary Organisations">
                    <img src="https://storage.googleapis.com/scvo-assets/scvo-frontend/assets/images/logos/scvo.png" alt="Scottish Council for Voluntary Organisations" />
                  </a>
                </div>
              </div>
            </div>
          </div>
        </div>
      </footer>
    </div>
    <div class="mdc-snackbar mdc-elevation--z10"
         aria-live="assertive"
         aria-atomic="true"
         aria-hidden="true"
         id="app-snackbar">
      <div class="mdc-snackbar__text"></div>
      <div class="mdc-snackbar__action-wrapper">
        <button type="button" class="mdc-snackbar__action-button"></button>
      </div>
    </div>
  </fundingscotland-site>
