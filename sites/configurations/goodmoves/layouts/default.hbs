</head>
<body class="mdc-theme--site-bg {{#if request.params.path}}{{replace request.params.path '_' ' '}}{{else}}home{{/if}}">
  <goodmoves-site>
    <div class="mdc-typography">
      {{#ifAny request.params.query.debug (compare request.url.hostname "goodmoves.local") ~}}
        {{#if errors.[0]}}
          <div style="z-index:100;position:fixed;top:0;right:0;padding:5px;background-color:#ff0000;color:#ffffff;">
            Route Task Errors (F12)
          </div>
        {{/if}}
        {{#if logs.[0]}}
          <div style="z-index:100;position:fixed;bottom:0;right:0;padding:5px;background-color:#0000ff;color:#ffffff;"
               onclick="document.querySelector('#route-debug-log').style.display='block';">
            Route Log
          </div>
          <div style="z-index:100;position:fixed;bottom:0;left:0;right:0;padding:5px;background-color:#ededed;color:#000000;overflow:auto;height:50vh;display:none;border-top:1px solid #cccccc;" id="route-debug-log">
            <a style="position:fixed;right:25px;" onclick="document.querySelector('#route-debug-log').style.display='none';">x</a>
<pre style="white-space: pre-wrap;">
{{~#each logs}}
{{{this}}}

{{/each~}}
</pre>
          </div>
        {{/if}}
      {{/ifAny}}
      <header class="mdc-top-app-bar mdc-elevation--z5 top-bar-stuck" id="top-bar">
        <div class="container-fluid">
          <div class="mdc-top-app-bar__row mdc-top-app-bar__upper">
            <section class="mdc-top-app-bar__section mdc-top-app-bar__section--align-start">
              {{#unless @root.route.metaData.hideSearch ~}}
              {{#compare @root.route.name "!==" "home" ~}}
              <button class="mdc-top-app-bar__navigation-icon hide-lg" id="sidebar-button" title="Search">
                <span class="far fa-fw fa-2x fa-search"></span>
              </button>
              {{/compare ~}}
              {{/unless ~}}
              <span class="mdc-top-app-bar__title">
                {{> logo link="true"}}
              </span>
            </section>
            <section class="mdc-top-app-bar__section mdc-top-app-bar__section--align-end">
              <button class="mdc-top-app-bar__navigation-icon hide-lg" data-drawer-target="#mobile-menu">
                <span class="far fa-fw fa-2x fa-bars"></span>
              </button>
              {{#>components_tab_bar
                  extraClasses="mdc-tab-bar__auto-width hide-sm hide-md"
                  id="upper-tab-bar"}}
                {{#if @root.data.auth ~}}
                  {{#each @root.context.metaData.menus.upper-signed-in ~}}
                    {{>components_tab
                        url=(stripTrailingSlash path)
                        active=(regexMatch @root.request.url.pathname route "gi")
                        label=label}}
                  {{/each ~}}
                {{else}}
                  {{#each @root.context.metaData.menus.upper-not-signed-in ~}}
                    {{>components_tab
                        url=(stripTrailingSlash path)
                        active=(regexMatch @root.request.url.pathname route "gi")
                        label=label}}
                  {{/each ~}}
                {{/if ~}}
                {{#each @root.context.metaData.menus.upper ~}}
                  {{>components_tab
                      url=(stripTrailingSlash path)
                      active=(regexMatch @root.request.url.pathname route "gi")
                      label=label}}
                {{/each ~}}
              {{/components_tab_bar}}
            </section>
          </div>
          <div class="mdc-top-app-bar__row mdc-top-app-bar__lower hide-sm hide-md">
            <section class="mdc-top-app-bar__section mdc-top-app-bar__section--align-start mdc-top-app-bar__main">
              {{#>components_tab_bar
                  extraClasses="mdc-tab-bar__auto-width"
                  id="main-tab-bar"}}
                {{#each @root.context.metaData.menus.main ~}}
                  {{>components_tab
                      url=(stripTrailingSlash path)
                      labelClasses="mdc-theme--on-primary"
                      active=(regexMatch @root.request.url.pathname route "gi")
                      label=label}}
                {{/each ~}}
                {{#each @root.context.metaData.menus.main-extra ~}}
                  {{>components_tab
                      url=(stripTrailingSlash path)
                      labelClasses="mdc-theme--on-primary"
                      active=(regexMatch @root.request.url.pathname route "gi")
                      label=label}}
                {{/each ~}}
              {{/components_tab_bar}}
            </section>
            <section class="mdc-top-app-bar__section mdc-top-app-bar__section--align-end mdc-top-app-bar__user">
              {{#if @root.data.auth ~}}
                {{#>components_anchored_menu
                    buttonClasses="mdc-theme--on-primary"
                    imageUrl=(default @root.data.auth.photoURL false)
                    imageClasses="far fa-fw user-avatar"
                    iconClasses=(if (if @root.data.auth.photoURL false else=true) "far fa-fw fa-user-circle" else=false)
                    buttonLabel=@root.data.auth.email
                    arrowClasses="far fa-fw fa-angle-down"}}
                  {{#each @root.context.metaData.menus.user-signed-in ~}}
                    <a class="mdc-list-item {{#if (regexMatch @root.request.url.pathname route "gi")}} mdc-list-item--activated{{/if}}" href="{{stripTrailingSlash path}}" role="menuitem">
                      {{#if icon ~}}
                      <span class="far fa-{{icon}} mdc-list-item__graphic" aria-hidden="true"></span>
                      {{/if ~}}
                      {{{label}}}
                    </a>
                  {{/each ~}}
                {{/components_anchored_menu}}
              {{else}}
                {{#>components_tab_bar
                    extraClasses="mdc-tab-bar__auto-width"
                    id="user-not-signed-in-tab-bar"}}
                  {{#each @root.context.metaData.menus.user-not-signed-in ~}}
                    {{>components_tab
                        url=(stripTrailingSlash path)
                        labelClasses="mdc-theme--on-primary"
                        active=(regexMatch @root.request.url.pathname route "gi")
                        label=label}}
                  {{/each}}
                {{/components_tab_bar}}
              {{/if ~}}
            </section>
          </div>
        </div>
      </header>

      <aside class="mdc-drawer mdc-drawer--modal" id="mobile-menu" data-mdc-auto-init="MDCDrawer">
        <div class="mdc-drawer__content">
          <nav class="mdc-list">
            <a class="mdc-list-item {{#compare @root.request.url.pathname "/"}} mdc-list-item--activated{{/compare}}"
               href="/" aria-selected="true" tabindex="1">
              <span class="far fa-home mdc-list-item__graphic" aria-hidden="true"></span>
              Home
            </a>
            {{#each @root.context.metaData.menus.main ~}}
            <a class="mdc-list-item {{#if (regexMatch @root.request.url.pathname route "gi")}} mdc-list-item--activated{{/if}}"
               href="{{stripTrailingSlash path}}" {{#if (regexMatch @root.request.url.pathname route "gi")}}aria-selected="true"{{else}}aria-hidden="true"{{/if}}>
              {{#if icon ~}}
              <span class="far fa-{{icon}} mdc-list-item__graphic" aria-hidden="true"></span>
              {{/if ~}}
              {{{label}}}
            </a>
            {{/each ~}}
            {{#each @root.context.metaData.menus.main-extra ~}}
            <a class="mdc-list-item {{#if (regexMatch @root.request.url.pathname route "gi")}} mdc-list-item--activated{{/if}}"
               href="{{stripTrailingSlash path}}" {{#if (regexMatch @root.request.url.pathname route "gi")}}aria-selected="true"{{else}}aria-hidden="true"{{/if}}>
              {{#if icon ~}}
              <span class="far fa-{{icon}} mdc-list-item__graphic" aria-hidden="true"></span>
              {{/if ~}}
              {{{label}}}
            </a>
            {{/each ~}}
            <div role="separator" class="mdc-list-divider"></div>
            {{#if @root.data.auth ~}}
            {{#each @root.context.metaData.menus.user-signed-in ~}}
            {{#compare path "===" "/" ~}}
            {{else}}
            <a class="mdc-list-item {{#if (regexMatch @root.request.url.pathname route "gi")}} mdc-list-item--activated{{/if}}"
               href="{{stripTrailingSlash path}}" {{#if (regexMatch @root.request.url.pathname route "gi")}}aria-selected="true"{{else}}aria-hidden="true"{{/if}}>
              {{#if icon ~}}
              <span class="far fa-{{icon}} mdc-list-item__graphic" aria-hidden="true"></span>
              {{/if ~}}
              {{{label}}}
            </a>
            {{/compare ~}}
            {{/each ~}}
            {{else}}
            {{#each @root.context.metaData.menus.user-not-signed-in ~}}
            <a class="mdc-list-item {{#if (regexMatch @root.request.url.pathname route "gi")}} mdc-list-item--activated{{/if}}"
               href="{{stripTrailingSlash path}}" {{#if (regexMatch @root.request.url.pathname route "gi")}}aria-selected="true"{{else}}aria-hidden="true"{{/if}}>
              {{#if icon ~}}
              <span class="far fa-{{icon}} mdc-list-item__graphic" aria-hidden="true"></span>
              {{/if ~}}
              {{{label}}}
            </a>
            {{/each ~}}
            {{/if ~}}
          </nav>
        </div>
      </aside>

      <div class="mdc-drawer-scrim"></div>

      <main class="mdc-theme--background mdc-toolbar-fixed-adjust {{#if layoutParts.sidebar}}drawer-container-permanent{{else}}drawer-container-temporary{{/if}}">
        {{#if layoutParts.sidebar }}
        <aside class="mdc-drawer mdc-elevation--z3 mdc-drawer--occasional" id="sidebar-temporary" data-menu-button="#sidebar-button">
          {{{layoutParts.sidebar}}}
        </aside>
        <div class="mdc-drawer-scrim"></div>
        {{/if}}
        {{{layoutParts.body}}}
      </main>

      <footer class="mdc-layout__footer">
        <div class="footer-main">
          <div class="container-fluid">
            <div class="mdc-layout-grid">
              <div class="mdc-layout-grid__inner">
                <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-3-desktop mdc-layout-grid__cell--span-4-tablet mdc-layout-grid__cell--span-4-phone contact">
                  <h3>Contact</h3>
                  <ul class="fa-ul">
                    <li>
                      <span class="fa-li"><span class="fas fa-envelope"></span></span>
                      <a href="mailto:enquiries@goodmoves.org.uk">enquiries@goodmoves.org.uk</a>
                    </li>
                    <li>
                      <span class="fa-li"><span class="fas fa-phone"></span></span>
                      <a href="tel:+448000192149">0800&nbsp;0192&nbsp;149</a>
                    </li>
                    <li>
                      <span class="fa-li"><span class="fab fa-twitter"></span></span>
                      <a href="https://twitter.com/goodmovesjobs" target="_blank">Twitter</a>
                    </li>
                  </ul>
                </div>
                <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-3-desktop mdc-layout-grid__cell--span-4-tablet mdc-layout-grid__cell--span-4-phone help">
                  <h3>Help</h3>
                  <ul class="mdc-list">
                    {{#each @root.context.metaData.menus.footer-help ~}}
                    <li class="mdc-list-item">
                      <a href="{{path}}">{{{label}}}</a>
                    </li>
                    {{/each ~}}
                    <div id="google_translate_element"></div>
                  </ul>
                </div>
                <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-3-desktop mdc-layout-grid__cell--span-4-tablet mdc-layout-grid__cell--span-4-phone info">
                  <h3>Information</h3>
                  <ul class="mdc-list">
                    {{#each @root.context.metaData.menus.footer-info ~}}
                    <li class="mdc-list-item">
                      <a href="{{path}}">{{{label}}}</a>
                    </li>
                    {{/each ~}}
                  </ul>
                </div>
                <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-3-desktop mdc-layout-grid__cell--span-4-tablet mdc-layout-grid__cell--span-4-phone about">
                  <h3>About</h3>
                  <p>
                    <strong>Goodmoves</strong> is the premier careers hub for the civil society, charity and voluntary sector &ndash; run by the <a href="https://scvo.org">Scottish Council for Voluntary Organisations</a>.
                  </p>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="footer-copyright">
          <div class="container-fluid">
            <div class="mdc-layout-grid">
              <div class="mdc-layout-grid__inner">
                <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-10-desktop mdc-layout-grid__cell--span-6-tablet mdc-layout-grid__cell--span-4-phone">
                  <p>
                    &copy; {{momentFormat (moment) "YYYY"}}. The Scottish Council for Voluntary Organisations (SCVO) is a Scottish Charitable Incorporated Organisation.<br />
                    Charity registered in Scotland <a href="https://www.oscr.org.uk/about-charities/search-the-register/charity-details?number=SC003558">SC003558</a>. Registered office Mansfield Traquair Centre, 15 Mansfield Place, Edinburgh EH3 6BB.
                  </p>
                </div>
                <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-2-desktop mdc-layout-grid__cell--span-2-tablet mdc-layout-grid__cell--span-4-phone">
                  <a href="https://scvo.org" class="brand-logo" title="Scottish Council for Voluntary Organisations">
                    <img src="https://storage.googleapis.com/scvo-assets/scvo-frontend/assets/images/logos/scvo.png" alt="Scottish Council for Voluntary Organisations" />
                  </a>
                </div>
              </div>
            </div>
          </div>
        </div>
      </footer>
    </div>
    <div class="mdc-snackbar mdc-elevation--z10"
         aria-live="assertive"
         aria-atomic="true"
         aria-hidden="true"
         id="app-snackbar">
      <div class="mdc-snackbar__text"></div>
      <div class="mdc-snackbar__action-wrapper">
        <button type="button" class="mdc-snackbar__action-button"></button>
      </div>
    </div>
  </goodmoves-site>

