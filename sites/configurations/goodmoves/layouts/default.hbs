  {{#compare @root.request.url.hostname "goodmoves.local" ~}}
    <link type="text/css" rel="stylesheet" href="/assets/goodmoves/main.css">
  {{else}}
    <link type="text/css" rel="stylesheet" href="https://storage.googleapis.com/scvo-assets/goodmoves/main-{{@root.context.metaData.assetsVersion}}.css">
