{{#if @root.data.primaryResponse.hits.hits.[0] ~}}
{{#with @root.data.primaryResponse.hits.hits.[0]._source ~}}
<title>SCVO – Funding update – {{momentFormat (moment (lastItem (split (atob ../request.params.range) "-")) "x") "Do MMMM YYYY"}}</title>
<meta property="og:title" content="SCVO – Funding update – {{momentFormat (moment (lastItem (split (atob ../request.params.range) "-")) "x") "Do MMMM YYYY"}}" data-dynamic="true">
{{#ifAny og_image ../og_image ~}}
<meta property="og:image" content="{{default og_image ../og_image}}" data-dynamic="true">
{{/ifAny ~}}
{{#if rendered.json_ld ~}}
<script type="application/ld+json" data-dynamic="true">
{{{rendered.json_ld}}}
</script>
{{/if ~}}
{{/with ~}}
{{else}}
<title>SCVO – Scottish Council for Voluntary Organisations</title>
<meta property="og:title" content="SCVO – Scottish Council for Voluntary Organisations" data-dynamic="true">
{{/if ~}}
