{
  "name": "home",
  "pattern": "/",
  "metaData": {
    "title": "Home"
  },
  "tasks": [
    {
      "name": "primaryResponse",
      "taskModule": "elasticsearch",
      "config": {
        "connectionStringTemplate": "{$../../elasticsearch-url.txt}",
        "elasticsearchConfig": {
          "apiVersion": "5.4"
        },
        "queryTemplates": [
          {
            "name": "first_query",
            "index": "web-content-production",
            "type": "goodmoves-job",
            "template": ">context.metaData.handlebars.queries.pages_none"
          },
          {
            "name": "second_query",
            "index": "web-content-production",
            "type": "goodmoves-job",
            "template": ">context.metaData.handlebars.queries.pages_none"
          }
        ]
      },
      "renderer": "handlebars"
    },
    {
      "name": "destination",
      "taskModule": "renderLayout",
      "renderer": "handlebars",
      "config": {
        "logic": {
          "if": [
            "default"
          ]
        },
        "output": "body",
        "layouts": {
          "default": {
            "layout": ">context.metaData.handlebars.layouts.default",
            "parts": {
              "head": ">context.metaData.handlebars.templates.head-tags",
              "body": ">context.metaData.handlebars.templates.home",
              "foot": ""
            },
            "contentType": "text/html"
          }
        }
      }
    }
  ]
}